    "a=float(input('請輸入你的體重(公斤):'))\n",
    "b=float(input('請輸入你的身高(公尺):'))\n",
    "BMI=a/(b*b)\n",
    "if BMI<18.5:\n",
    "    print('體重過輕')\n",
    "elif 18.5<=BMI<24:\n",
    "    print('正常範圍')\n",
    "elif 24<=BMI<27:\n",
    "    print('過胖')\n",
    "elif 27<=BMI<30:\n",
    "    print('輕度肥胖')\n",
    "elif 30<=BMI<35:\n",
    "    print('中度肥胖')\n",
    "else:\n",
    "    print('肥死')"
